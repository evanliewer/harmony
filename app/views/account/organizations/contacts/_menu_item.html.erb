<% if can? :read, Organizations::Contact.new(team: current_team) %>
  <%= render 'account/shared/menu/item', {
    url: main_app.polymorphic_path([:account, current_team, :organizations_contacts]),
    label: t('organizations/contacts.navigation.label'),
  } do |p| %>
    <% p.icon do %>
      <i class="<%= t('organizations/contacts.navigation.icon') %>"></i>
    <% end %>
  <% end %>
<% end %>
