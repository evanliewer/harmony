<% if can? :read, Notifications::ArchiveAction.new(team: current_team) %>
  <%= render 'account/shared/menu/item', {
    url: main_app.polymorphic_path([:account, current_team, :notifications_archive_actions]),
    label: t('notifications/archive_actions.navigation.label'),
  } do |p| %>
    <% p.content_for :icon do %>
      <i class="<%= t('notifications/archive_actions.navigation.icon') %>"></i>
    <% end %>
  <% end %>
<% end %>
