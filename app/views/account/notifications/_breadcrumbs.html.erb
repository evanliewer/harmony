<% notification ||= @notification %>
<% team ||= @team || notification&.team %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, team, :notifications] %>
<% if notification&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: notification.label_string, url: [:account, notification] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'notifications' %>
