<% if can? :read, Question.new(team: current_team) %>
  <%= render 'account/shared/menu/item', {
    url: main_app.polymorphic_path([:account, current_team, :questions]),
    label: t('questions.navigation.label'),
  } do |p| %>
    <% p.icon do %>
      <i class="<%= t('questions.navigation.icon') %>"></i>
    <% end %>
  <% end %>
<% end %>
