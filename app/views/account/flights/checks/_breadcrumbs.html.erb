<% check ||= @check %>
<% team ||= @team || check&.team %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, team, :flights_checks] %>
<% if check&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: check.label_string, url: [:account, check] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'flights/checks' %>
